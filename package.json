{
  "name": "hare-networked",
  "version": "1.0.0",
  "description": "hare-networked",
  "author": "strangerintheq",
  "private": true,
  "scripts": {
    "build-client": "esbuild client/index.tsx --bundle --outdir=static --loader:.glsl=text --define:process.env.NODE_ENV=\\\"development\\\"",
    "prebuild": "rimraf dist",
    "build-server": "nest build",
    "start": "npm run build-server && npm run build-client && node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch"
  },
  "dependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-socket.io": "^7.6.15",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/websockets": "^7.6.15",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "simplex-noise": "^2.4.0",
    "esbuild": "^0.8.21",
    "drei": "^2.2.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-three-fiber": "^5.3.22",
    "three": "^0.127.0",
    "troika-three-text": "^0.40.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.3.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.36",
    "@types/socket.io": "^2.1.13",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
